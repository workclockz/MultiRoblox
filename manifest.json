{
    "name": "MultiRoblox",
    "description": "MultiRoblox will help you change your account easily!",
    "manifest_version": 3,
    "permissions": [
        "cookies",
        "storage"
    ],
    "host_permissions": [
        "*://*.roblox.com/*"
    ],
    "update_url": "https://clients2.google.com/service/update2/crx",
    "version": "1.1.6",
    "icons": {
        "128": "icon.png"
    },
    "action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    }
}